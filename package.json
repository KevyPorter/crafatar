{
  "name": "crafatar",
  "version": "0.0.1",
  "private": true,
  "author": "Jake0oo0",
  "description": "A Minecraft avatar service with support for avatars, skins, and even renders!",
  "contributors": [
    {
      "name": "jomo"
    }
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/Jake0oo0/crafatar"
  },
  "engines": {
    "iojs": "2.0.x"
  },
  "issues": {
    "url": "https://github.com/Jake0oo0/crafatar/issues"
  },
  "keywords": [
    "minecraft",
    "avatar"
  ],
  "scripts": {
    "postinstall": "cp 'modules/config.example.js' 'modules/config.js'",
    "start": "forever -l logs/log.log -o logs/out.log -e logs/error.log -p ./ -a --minUptime 8000 --spinSleepTime 1500 server.js",
    "test": "istanbul cover ./node_modules/mocha/bin/_mocha --report lcovonly -- -R spec && cat ./coverage/lcov.info | ./node_modules/coveralls/bin/coveralls.js && rm -rf ./coverage"
  },
  "dependencies": {
    "canvas": "crafatar/node-canvas",
    "coveralls": "^2.11.2",
    "forever": "0.14.1",
    "istanbul": "^0.3.2",
    "jade": "~1.8.2",
    "lwip": "crafatar/lwip",
    "mime": "1.2.11",
    "mocha": "^2.1.0",
    "mocha-lcov-reporter": "0.0.1",
    "node-df": "0.1.1",
    "redis": "0.12.1",
    "request": "2.51.0"
  }
}
